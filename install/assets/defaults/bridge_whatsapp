WHATSAPP_LOG_PATH=${WHATSAPP_LOG_PATH:-"${LOG_PATH}"}
WHATSAPP_LOG_FILE=${WHATSAPP_LOG_FILE:-"whatsapp.log"}
WHATSAPP_LOG_LEVEL=${WHATSAPP_LOG_LEVEL:-"INFO"}
WHATSAPP_DATA_PATH=${WHATSAPP_DATA_PATH:-"${DATA_PATH}/whatsapp"}
WHATSAPP_REGISTRATION_FILE=${WHATSAPP_REGISTRATION_FILE:-"registration.yaml"}
WHATSAPP_REGISTRATION_PATH=${WHATSAPP_REGISTRATION_PATH:-"${REGISTRATION_PATH}"}