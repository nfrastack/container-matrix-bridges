#!/command/with-contenv bash

GOOGLECHAT_CONFIG_FILE=${GOOGLECHAT_CONFIG_FILE:-"googlechat.yaml"}
GOOGLECHAT_CONFIG_PATH=${GOOGLECHAT_CONFIG_PATH:-"${CONFIG_PATH}"}
GOOGLECHAT_DATA_PATH=${GOOGLECHAT_DATA_PATH:-"${DATA_PATH}/googlechat"}
GOOGLECHAT_LOG_FILE=${GOOGLECHAT_LOG_FILE:-"googlechat.log"}
GOOGLECHAT_LOG_LEVEL=${GOOGLECHAT_LOG_LEVEL:-"INFO"}
GOOGLECHAT_LOG_PATH=${GOOGLECHAT_LOG_PATH:-"${LOG_PATH}"}
GOOGLECHAT_REGISTRATION_FILE=${GOOGLECHAT_REGISTRATION_FILE:-"registration.yaml"}
GOOGLECHAT_REGISTRATION_PATH=${GOOGLECHAT_REGISTRATION_PATH:-"${REGISTRATION_PATH}"}